{
  "name": "ayahbot",
  "version": "2.0.1",
  "description": "A Daily Schedulable Quran Bot",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "bash build.sh",
    "build:make": "make",
    "test": "jest",
    "teste": "jest --forceExit",
    "testd": "node -r dotenv/config node_modules/jest/bin/jest.js --detectOpenHandles",
    "lint": "eslint .",
    "format": "prettier --write .",
    "start:prod": "node --experimental-specifier-resolution=node -r dotenv/config index",
    "dev": "tsm -r dotenv/config index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AyahBot-Dev/AyahBot-Discord.git"
  },
  "author": "AyahBot-Dev",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/AyahBot-Dev/AyahBot-Discord/issues"
  },
  "homepage": "https://github.com/AyahBot-Dev/AyahBot-Discord#readme",
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "axios": "^0.27.2",
    "discord-api-types": "^0.32.1",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "firebase-admin": "^10.2.0",
    "inversify": "^6.0.1",
    "moment-timezone": "^0.5.34",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.5",
    "pm2": "^5.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.5.0",
    "tsm": "^2.2.1",
    "typescript": "^4.6.4"
  }
}
